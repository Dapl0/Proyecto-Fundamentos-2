public class BichoEspacial extends Bicho{

    private int salud = 30;

    public BichoEspacial() {
    }

    public BichoEspacial(int salud) {
        this.salud=salud;
    }

    public int getSalud() {
        return salud;
    }

    public void setSalud(int salud) {
        this.salud = salud;
    }  

    @Override
    public String toString() {
        return "BE-"+ this.salud;
    }
}  
