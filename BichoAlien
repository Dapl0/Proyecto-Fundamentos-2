public class BichoAlien extends Bicho{

    private int salud = 20;

    public BichoAlien() {
    }

    public BichoAlien(int salud) {
        this.salud=salud;
    }

    public int getSalud() {
        return salud;
    }

    public void setSalud(int salud) {
        this.salud = salud;
    } 

    @Override
    public String toString() {
        return "BA-"+ this.salud;
    }
}
